<?xml version="1.0" encoding="UTF-8" ?>
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
    <Toolbar class="voFormFooterToolbar">
        <Button text="{i18n>Cancel}" type="Default" class="voFormBtnDefault sapUiTinyMarginBegin" visible="{oButtonmodel>/cancelButtonVisible}" press="onCancel"/>
         <Button text="{i18n>Back}" type="Default" class="voFormBtnDefault sapUiTinyMarginBegin" visible="{oButtonmodel>/backButtonVisible}" press="onDialogBackButton"/>
        <ToolbarSpacer />
         <Button text="{i18n>Preview}" type="Default" class="voFormBtnDefault sapUiTinyMarginBegin" visible="{oButtonmodel>/previewButtonVisible}" press="onPrintVendorDetails"/>
        <Button text="Save As Draft" type="Emphasized" class="voFormBtnEmphasized sapUiTinyMarginBegin" visible="{oButtonmodel>/saveButtonVisible}" press="SaveVendorDetails"/>
        <Button text="{i18n>Next}" type="Emphasized" class="voFormBtnEmphasized sapUiTinyMarginBegin" visible="{oButtonmodel>/nextButtonVisible}" press="onDialogNextButton" />
        <Button text="{i18n>Submit}" type="Emphasized" class="voFormBtnEmphasized sapUiTinyMarginBegin" visible="{oButtonmodel>/submitButtonVisible}" press="postVendorDetails" />
    </Toolbar>
</core:FragmentDefinition>