<?xml version="1.0" encoding="UTF-8" ?>
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
    <VBox >
    <VBox id="companyCodeInformationVbox" items="{oVendorDetailModel>/companyCodeInfo}">
        <VBox class="vendorRegistrationFormSection" id="panel1" >
            <Panel expandable="true" expanded="true" headerText="ACCOUNTING &amp; CORRESPONDENCE DETAILS 1" class=" vendorRegistrationFormPanel">
                <VBox>
                    <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Company Code" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oVendorDetailModel>companyCode}"
                            >
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Reconciliation Account" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>reconcilliationAccount}" maxLength="40" change="oAddressChange" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street2" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Interest Ind." class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>interestInd}" maxLength="40" change="oAddressChange" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street3" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="W.Tax Code " class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>wTaxCode}" maxLength="40" change="oAddressChange" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street4" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="WH. Tax Country" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>whTaxCountry}" maxLength="40" change="oAddressChange" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street5" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Recipient Type" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oVendorDetailModel>/recipientType}"
                                maxLength="40"
                                valueState="{oMandatoryModel>/vendorAddress/cityState}"
                                valueStateText="{i18n>ManFields}"
                                change="oAddressChange"
                                enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                                liveChange="onAddingmandvalue"
                            >
                                <customData>
                                    <core:CustomData key="target" value="city" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Exemption No" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oVendorDetailModel>/exemptionNo}"
                                liveChange="validatePostalCode"
                                valueState="{oMandatoryModel>/vendorAddress/cityPostalCodeState}"
                                valueStateText="{i18n>ManFields}"
                                enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            >
                                <customData>
                                    <core:CustomData key="target" value="cityPostalCode" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Valid Until" class="voGridLabel" required="true" />
                            <!-- Date picker  -->
                            <Input class="voInputText" value="{oVendorDetailModel>validUntil}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Exemption Authority" class="voGridLabel" required="true" />
                            <Input class="voInputText" value="{oVendorDetailModel>exemptionAuthority}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Payment Terms" class="voGridLabel" />
                            <ComboBox class="voComboBox" width="100%" selectedKey="{oVendorDetailModel>/vendorType}" enabled="true" items="{oDropdownModel>/vendortype}">
                                <core:Item key="{oDropdownModel>code}" text="{oDropdownModel>description}" />
                            </ComboBox>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Tolerance Group" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>/vendorAddress/website}" maxLength="2048" change="oAddressChange" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="website" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Payment Methods" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Payment Block" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>/fax}" maxLength="40" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="House Bank" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>/vendorAddress/website}" maxLength="2048" change="oAddressChange" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="website" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Grouping Key" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                    </l:Grid>
                    <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Accounting Clerk" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>/vendorAddress/street4}" maxLength="40" change="oAddressChange" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street4" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Accounting Clerkâ€™s Fax" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>/vendorAddress/street5}" maxLength="40" change="oAddressChange" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street5" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Acting Clerkâ€™s Telephone" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oVendorDetailModel>/vendorAddress/city}"
                                maxLength="40"
                                valueState="{oMandatoryModel>/vendorAddress/cityState}"
                                valueStateText="{i18n>ManFields}"
                                change="oAddressChange"
                                enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                                liveChange="onAddingmandvalue"
                            >
                                <customData>
                                    <core:CustomData key="target" value="city" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Clerk at  Supplier" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oVendorDetailModel>/vendorAddress/cityPostalCode}"
                                liveChange="validatePostalCode"
                                valueState="{oMandatoryModel>/vendorAddress/cityPostalCodeState}"
                                valueStateText="{i18n>ManFields}"
                                enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            >
                                <customData>
                                    <core:CustomData key="target" value="cityPostalCode" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Planning Group" class="voGridLabel" required="true" />
                            <Input class="voInputText" value="{oVendorDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Minority Indicator" class="voGridLabel" required="true" />
                            <Input class="voInputText" value="{oVendorDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Clerk Email Address" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>/fax}" maxLength="40" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                        </VBox>
                    </l:Grid>
                    <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Reconciliation Account in General Ledger" class="voGridLabel" />
                            <Input class="voInputText" value="{oVendorDetailModel>/vendorAddress/street4}" maxLength="40" change="oAddressChange" enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street4" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Check Flag for Double Invoices or Credit Memo" class="voGridLabel" />
                            <ComboBox class="voComboBox" width="100%" selectedKey="{oVendorDetailModel>/vendorType}" enabled="true" items="{oDropdownModel>/vendortype}">
                                <core:Item key="{oDropdownModel>code}" text="{oDropdownModel>description}" />
                            </ComboBox>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Company Code Deletion Block" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oVendorDetailModel>/vendorAddress/city}"
                                maxLength="40"
                                valueState="{oMandatoryModel>/vendorAddress/cityState}"
                                valueStateText="{i18n>ManFields}"
                                change="oAddressChange"
                                enabled="{parts: ['oVendorDetailModel>/notExtended','oVendorDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                                liveChange="onAddingmandvalue"
                            >
                                <customData>
                                    <core:CustomData key="target" value="city" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="Sort Key for  Payment Assignment" class="voGridLabel" required="true" />
                            <ComboBox class="voComboBox" width="100%" selectedKey="{oVendorDetailModel>/vendorType}" enabled="true" items="{oDropdownModel>/vendortype}">
                                <core:Item key="{oDropdownModel>code}" text="{oDropdownModel>description}" />
                            </ComboBox>
                        </VBox>
                    </l:Grid>
                </VBox>
            </Panel>
        </VBox>
        </VBox>
        <VBox alignItems="End" justifyContent="End">
            <Button text="Add Section" icon="sap-icon://positive" press="onClickAddSectionCompanyCodeInformation" />
        </VBox>
    </VBox>
</core:FragmentDefinition>
