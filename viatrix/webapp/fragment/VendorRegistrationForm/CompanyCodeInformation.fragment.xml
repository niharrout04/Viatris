<?xml version="1.0" encoding="UTF-8" ?>
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
    <VBox class="vendorRegistrationFormSection">
        <Panel expandable="true" expanded="true" headerText="ACCOUNTING &amp; CORRESPONDENCE DETAILS 1" class=" vendorRegistrationFormPanel">
            <VBox>
                <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Company Code" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street}"
                            maxLength="60"
                            valueState="{oMandatoryModel>/vendorAddress/streetState}"
                            valueStateText="{i18n>ManFields}"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            liveChange="onAddingmandvalue"
                        >
                            <customData>
                                <core:CustomData key="target" value="street" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Reconciliation Account" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street2}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="street2" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Interest Ind." class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street3}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="street3" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="W.Tax Code " class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street4}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="street4" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="WH. Tax Country" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street5}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="street5" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Recipient Type" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/city}"
                            maxLength="40"
                            valueState="{oMandatoryModel>/vendorAddress/cityState}"
                            valueStateText="{i18n>ManFields}"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            liveChange="onAddingmandvalue"
                        >
                            <customData>
                                <core:CustomData key="target" value="city" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Exemption No" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/cityPostalCode}"
                            liveChange="validatePostalCode"
                            valueState="{oMandatoryModel>/vendorAddress/cityPostalCodeState}"
                            valueStateText="{i18n>ManFields}"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="cityPostalCode" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Valid Until" class="voGridLabel" required="true" />
                        <ComboBox
                            class="sigComboBox"
                            width="100%"
                            items="{oDropdownModel>/country}"
                            change="onCountrySelect"
                            selectedKey="{oSupplierDetailModel>/vendorAddress/countryKey}"
                            valueState="{oMandatoryModel>/vendorAddress/countryKeyState}"
                            valueStateText="{i18n>ManFields}"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            showSecondaryValues="true"
                            selectionChange="onAddingmandvalue"
                        >
                            <core:ListItem key="{oDropdownModel>countryKey}" text="{oDropdownModel>countryKey}" additionalText="{oDropdownModel>countryName}" />
                        </ComboBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Exemption Authority" class="voGridLabel" required="true" />
                        <HBox renderType="Bare">
                            <ComboBox
                                class="sigComboBox"
                                width="50%"
                                items="{oDropdownModel>/phoneCountryCode}"
                                showSecondaryValues="true"
                                change="onChangeCountryCode"
                                selectedKey="{oSupplierDetailModel>/countryCode}"
                                valueState="{oMandatoryModel>/phoneCountrycodeState}"
                                valueStateText="{i18n>ManFields}"
                                selectionChange="onAddingmandvalue"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            >
                                <core:ListItem key="{oDropdownModel>countryCode}" text="{oDropdownModel>countryCode}" additionalText="{oDropdownModel>countryKey}" />
                            </ComboBox>
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/mobileNumber}"
                                valueState="{oMandatoryModel>/mobileNumberState}"
                                valueStateText="{i18n>ManFields}"
                                liveChange="onPhoneNumberChange"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            />
                        </HBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Payment Terms" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/fax}" maxLength="40" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Tolerance Group" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/website}" maxLength="2048" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="website" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Payment Methods" class="voGridLabel" />
                        <ComboBox
                            class="sigComboBox"
                            width="100%"
                            items="{oDropdownModel>/region}"
                            selectedKey="{oSupplierDetailModel>/vendorAddress/region}"
                            change="oRegionChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <core:Item key="{oDropdownModel>regionKey}" text="{oDropdownModel>regionName}" />
                        </ComboBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Payment Block" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/fax}" maxLength="40" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="House Bank" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/website}" maxLength="2048" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="website" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Grouping Key" class="voGridLabel" />
                        <ComboBox
                            class="sigComboBox"
                            width="100%"
                            items="{oDropdownModel>/region}"
                            selectedKey="{oSupplierDetailModel>/vendorAddress/region}"
                            change="oRegionChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <core:Item key="{oDropdownModel>regionKey}" text="{oDropdownModel>regionName}" />
                        </ComboBox>
                    </VBox>
                </l:Grid>
                <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Accounting Clerk" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street4}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="street4" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Accounting Clerk’s Fax" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street5}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="street5" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Acting Clerk’s Telephone" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/city}"
                            maxLength="40"
                            valueState="{oMandatoryModel>/vendorAddress/cityState}"
                            valueStateText="{i18n>ManFields}"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            liveChange="onAddingmandvalue"
                        >
                            <customData>
                                <core:CustomData key="target" value="city" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Clerk at  Supplier" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/cityPostalCode}"
                            liveChange="validatePostalCode"
                            valueState="{oMandatoryModel>/vendorAddress/cityPostalCodeState}"
                            valueStateText="{i18n>ManFields}"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="cityPostalCode" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Planning Group" class="voGridLabel" required="true" />
                        <ComboBox
                            class="sigComboBox"
                            width="100%"
                            items="{oDropdownModel>/country}"
                            change="onCountrySelect"
                            selectedKey="{oSupplierDetailModel>/vendorAddress/countryKey}"
                            valueState="{oMandatoryModel>/vendorAddress/countryKeyState}"
                            valueStateText="{i18n>ManFields}"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            showSecondaryValues="true"
                            selectionChange="onAddingmandvalue"
                        >
                            <core:ListItem key="{oDropdownModel>countryKey}" text="{oDropdownModel>countryKey}" additionalText="{oDropdownModel>countryName}" />
                        </ComboBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Minority Indicator" class="voGridLabel" required="true" />
                        <HBox renderType="Bare">
                            <ComboBox
                                class="sigComboBox"
                                width="50%"
                                items="{oDropdownModel>/phoneCountryCode}"
                                showSecondaryValues="true"
                                change="onChangeCountryCode"
                                selectedKey="{oSupplierDetailModel>/countryCode}"
                                valueState="{oMandatoryModel>/phoneCountrycodeState}"
                                valueStateText="{i18n>ManFields}"
                                selectionChange="onAddingmandvalue"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            >
                                <core:ListItem key="{oDropdownModel>countryCode}" text="{oDropdownModel>countryCode}" additionalText="{oDropdownModel>countryKey}" />
                            </ComboBox>
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/mobileNumber}"
                                valueState="{oMandatoryModel>/mobileNumberState}"
                                valueStateText="{i18n>ManFields}"
                                liveChange="onPhoneNumberChange"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            />
                        </HBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Clerk Email Address" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/fax}" maxLength="40" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                    </VBox>
                </l:Grid>
                <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Reconciliation Account in General Ledger" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street4}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="street4" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Check Flag for Double Invoices or Credit Memo" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street5}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                            <customData>
                                <core:CustomData key="target" value="street5" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Company Code Deletion Block" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/city}"
                            maxLength="40"
                            valueState="{oMandatoryModel>/vendorAddress/cityState}"
                            valueStateText="{i18n>ManFields}"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            liveChange="onAddingmandvalue"
                        >
                            <customData>
                                <core:CustomData key="target" value="city" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Sort Key for  Payment Assignment" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/cityPostalCode}"
                            liveChange="validatePostalCode"
                            valueState="{oMandatoryModel>/vendorAddress/cityPostalCodeState}"
                            valueStateText="{i18n>ManFields}"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="cityPostalCode" />
                            </customData>
                        </Input>
                    </VBox>
                </l:Grid>
            </VBox>
        </Panel>
    </VBox>
</core:FragmentDefinition>
