<?xml version="1.0" encoding="UTF-8" ?>
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
    <VBox >
    <VBox id="companyCodeInformationVbox">
        <VBox class="vendorRegistrationFormSection"  >
            <Panel expandable="true" expanded="true" headerText="ACCOUNTING &amp; CORRESPONDENCE DETAILS 1" class=" vendorRegistrationFormPanel">
                <VBox>
                    <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>companyCode}" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/vendorAddress/street}"
                                maxLength="60"
                                valueState="{oMandatoryModel>/vendorAddress/streetState}"
                                valueStateText="{i18n>ManFields}"
                                change="oAddressChange"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                                liveChange="onAddingmandvalue"
                            >
                                <customData>
                                    <core:CustomData key="target" value="street" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>reconAccount}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street2}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street2" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>interestInd}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street3}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street3" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>wtax}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street4}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street4" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>wTaxCountry}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street5}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street5" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>recipientType}" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/vendorAddress/city}"
                                maxLength="40"
                                valueState="{oMandatoryModel>/vendorAddress/cityState}"
                                valueStateText="{i18n>ManFields}"
                                change="oAddressChange"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                                liveChange="onAddingmandvalue"
                            >
                                <customData>
                                    <core:CustomData key="target" value="city" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>exempNo}" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/vendorAddress/cityPostalCode}"
                                liveChange="validatePostalCode"
                                valueState="{oMandatoryModel>/vendorAddress/cityPostalCodeState}"
                                valueStateText="{i18n>ManFields}"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            >
                                <customData>
                                    <core:CustomData key="target" value="cityPostalCode" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>validUntil}" class="voGridLabel" required="true" />
                            <!-- Date picker  -->
                            <Input class="voInputText" value="{oSupplierDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>exempAuthority}" class="voGridLabel" required="true" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>payTerms}" class="voGridLabel" />
                            <ComboBox class="voComboBox" width="100%" selectedKey="{oSupplierDetailModel>/vendorType}" enabled="true" items="{oDropdownModel>/vendortype}">
                                <core:Item key="{oDropdownModel>code}" text="{oDropdownModel>description}" />
                            </ComboBox>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>toleranceGroup}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/website}" maxLength="2048" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="website" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>payMethods}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>payBlock}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/fax}" maxLength="40" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>houseBank}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/website}" maxLength="2048" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="website" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>groupKey}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                    </l:Grid>
                    <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>accClerk}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street4}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street4" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>accClerkFax}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street5}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street5" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>actClerkTele}" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/vendorAddress/city}"
                                maxLength="40"
                                valueState="{oMandatoryModel>/vendorAddress/cityState}"
                                valueStateText="{i18n>ManFields}"
                                change="oAddressChange"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                                liveChange="onAddingmandvalue"
                            >
                                <customData>
                                    <core:CustomData key="target" value="city" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>clerkSupplier}" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/vendorAddress/cityPostalCode}"
                                liveChange="validatePostalCode"
                                valueState="{oMandatoryModel>/vendorAddress/cityPostalCodeState}"
                                valueStateText="{i18n>ManFields}"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            >
                                <customData>
                                    <core:CustomData key="target" value="cityPostalCode" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>planGroup}" class="voGridLabel" required="true" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>minorIndicator}" class="voGridLabel" required="true" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/email}" enabled="true" type="Email" />
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>clerkMail}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/fax}" maxLength="40" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                        </VBox>
                    </l:Grid>
                    <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>reconAccGeneral}" class="voGridLabel" />
                            <Input class="voInputText" value="{oSupplierDetailModel>/vendorAddress/street4}" maxLength="40" change="oAddressChange" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}">
                                <customData>
                                    <core:CustomData key="target" value="street4" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>checkDoubleFlagMemo}" class="voGridLabel" />
                            <ComboBox class="voComboBox" width="100%" selectedKey="{oSupplierDetailModel>/vendorType}" enabled="true" items="{oDropdownModel>/vendortype}">
                                <core:Item key="{oDropdownModel>code}" text="{oDropdownModel>description}" />
                            </ComboBox>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>compDeletionBlock}" class="voGridLabel" required="true" />
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/vendorAddress/city}"
                                maxLength="40"
                                valueState="{oMandatoryModel>/vendorAddress/cityState}"
                                valueStateText="{i18n>ManFields}"
                                change="oAddressChange"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                                liveChange="onAddingmandvalue"
                            >
                                <customData>
                                    <core:CustomData key="target" value="city" />
                                </customData>
                            </Input>
                        </VBox>
                        <VBox class="sapUiSmallMarginTop">
                            <Label text="{i18n>sortkeyPay}" class="voGridLabel" required="true" />
                            <ComboBox class="voComboBox" width="100%" selectedKey="{oSupplierDetailModel>/vendorType}" enabled="true" items="{oDropdownModel>/vendortype}">
                                <core:Item key="{oDropdownModel>code}" text="{oDropdownModel>description}" />
                            </ComboBox>
                        </VBox>
                    </l:Grid>
                </VBox>
            </Panel>
        </VBox>
        </VBox>
        <VBox alignItems="End" justifyContent="End">
            <Button text="{i18n>addSection}" icon="sap-icon://positive" press="onClickAddSectionCompanyCodeInformation" />
        </VBox>
    </VBox>
</core:FragmentDefinition>
