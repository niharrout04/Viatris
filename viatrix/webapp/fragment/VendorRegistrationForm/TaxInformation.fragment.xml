<?xml version="1.0" encoding="UTF-8" ?>
<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout">
    <VBox>
        <Label text="{i18n>taxDetails}" class="vendorRegistrationFormSectionLabel" />
        <VBox class="vendorRegistrationFormSection sapUiTinyMarginTop">
            <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>taxNumber1}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/taxNo1}" enabled="true" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>taxNumber2}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/taxNo2}" enabled="true" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>taxNumber3}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/taxNo3}" enabled="true" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>taxNumber4}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/taxNo4}" enabled="true" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>taxNumber5}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/taxNo5}" enabled="true" type="Email" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>taxNumberType}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/taxNoType}" enabled="true" type="Email" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>taxType}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/taxType}" enabled="true" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>taxJurisdiction}" required="true" class="voGridLabel" />
                    <ComboBox class="voComboBox" width="100%" selectedKey="{oVendorDetailModel>/taxDto/taxJudrisdiction}" enabled="true" items="{oDropdownModel>/vendortype}">
                        <core:Item key="{oDropdownModel>code}" text="{oDropdownModel>description}" />
                    </ComboBox>
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>VATRegistrationNumber}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/vatRegNo}" enabled="true" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>transportZone}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/transportZone}" enabled="true" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>PODRelevant}}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/podRelevant}" enabled="true" type="Email" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>doB}}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/dob}" enabled="true" type="Email" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>industry}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/industry}" enabled="true" type="Email" />
                </VBox>
                <VBox class="sapUiSmallMarginTop">
                    <Label text="{i18n>vendorCalender}" class="voGridLabel" />
                    <Input class="voInputText" value="{oVendorDetailModel>/taxDto/vendorCalendar}" enabled="true" type="Email" />
                </VBox>
            </l:Grid>
        </VBox>
    </VBox>
</core:FragmentDefinition>
