<?xml version="1.0" encoding="UTF-8" ?>
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
    <VBox class="vendorRegistrationFormSection" >
        <Panel expandable="true" expanded="true" headerText="PURCHASING DETAILS 1" class="vendorRegistrationFormPanel">
            <VBox>
                <l:Grid defaultSpan="XL3 L3 M6 S12" hSpacing="2">
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Purchasing organization" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street}"
                            maxLength="60"
                            valueState="{oMandatoryModel>/vendorAddress/streetState}"
                            valueStateText="{i18n>ManFields}"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            liveChange="onAddingmandvalue"
                        >
                            <customData>
                                <core:CustomData key="target" value="street" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Purchase Order Currency" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street2}"
                            maxLength="40"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="street2" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Terms of Payment" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street3}"
                            maxLength="40"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="street3" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Incoterms" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street4}"
                            maxLength="40"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="street4" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Incoterms 2" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street5}"
                            maxLength="40"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="street5" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Price Data Control" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/city}"
                            maxLength="40"
                            valueState="{oMandatoryModel>/vendorAddress/cityState}"
                            valueStateText="{i18n>ManFields}"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            liveChange="onAddingmandvalue"
                        >
                            <customData>
                                <core:CustomData key="target" value="city" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Account with Vendor" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/cityPostalCode}"
                            liveChange="validatePostalCode"
                            valueState="{oMandatoryModel>/vendorAddress/cityPostalCodeState}"
                            valueStateText="{i18n>ManFields}"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="cityPostalCode" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Shipping condition" class="voGridLabel" required="true" />
                        <ComboBox
                            class="sigComboBox"
                            width="100%"
                            items="{oDropdownModel>/country}"
                            change="onCountrySelect"
                            selectedKey="{oSupplierDetailModel>/vendorAddress/countryKey}"
                            valueState="{oMandatoryModel>/vendorAddress/countryKeyState}"
                            valueStateText="{i18n>ManFields}"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            showSecondaryValues="true"
                            selectionChange="onAddingmandvalue"
                        >
                            <core:ListItem key="{oDropdownModel>countryKey}" text="{oDropdownModel>countryKey}" additionalText="{oDropdownModel>countryName}" />
                        </ComboBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Purchasing Group" class="voGridLabel" required="true" />
                        <HBox renderType="Bare">
                            <ComboBox
                                class="sigComboBox"
                                width="50%"
                                items="{oDropdownModel>/phoneCountryCode}"
                                showSecondaryValues="true"
                                change="onChangeCountryCode"
                                selectedKey="{oSupplierDetailModel>/countryCode}"
                                valueState="{oMandatoryModel>/phoneCountrycodeState}"
                                valueStateText="{i18n>ManFields}"
                                selectionChange="onAddingmandvalue"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            >
                                <core:ListItem key="{oDropdownModel>countryCode}" text="{oDropdownModel>countryCode}" additionalText="{oDropdownModel>countryKey}" />
                            </ComboBox>
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/mobileNumber}"
                                valueState="{oMandatoryModel>/mobileNumberState}"
                                valueStateText="{i18n>ManFields}"
                                liveChange="onPhoneNumberChange"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            />
                        </HBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Planned Delivery Date" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/fax}" maxLength="40" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Confirmation Control" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/website}"
                            maxLength="2048"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="website" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Unit of Measure Group" class="voGridLabel" />
                        <ComboBox
                            class="sigComboBox"
                            width="100%"
                            items="{oDropdownModel>/region}"
                            selectedKey="{oSupplierDetailModel>/vendorAddress/region}"
                            change="oRegionChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <core:Item key="{oDropdownModel>regionKey}" text="{oDropdownModel>regionName}" />
                        </ComboBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Rounding Profile" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/fax}" maxLength="40" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Purchase Organization Deletion Flag" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/website}"
                            maxLength="2048"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="website" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Indicator: GR- Based Invoice Verification" class="voGridLabel" />
                        <ComboBox
                            class="sigComboBox"
                            width="100%"
                            items="{oDropdownModel>/region}"
                            selectedKey="{oSupplierDetailModel>/vendorAddress/region}"
                            change="oRegionChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <core:Item key="{oDropdownModel>regionKey}" text="{oDropdownModel>regionName}" />
                        </ComboBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="ABC Indicator" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street4}"
                            maxLength="40"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="street4" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Responsible Sales Person" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street5}"
                            maxLength="40"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="street5" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Sales Person's Telephone Number" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/city}"
                            maxLength="40"
                            valueState="{oMandatoryModel>/vendorAddress/cityState}"
                            valueStateText="{i18n>ManFields}"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            liveChange="onAddingmandvalue"
                        >
                            <customData>
                                <core:CustomData key="target" value="city" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Order Acknowledgement Requirement" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/cityPostalCode}"
                            liveChange="validatePostalCode"
                            valueState="{oMandatoryModel>/vendorAddress/cityPostalCodeState}"
                            valueStateText="{i18n>ManFields}"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="cityPostalCode" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Automatic Purchase order Generation" class="voGridLabel" required="true" />
                        <ComboBox
                            class="sigComboBox"
                            width="100%"
                            items="{oDropdownModel>/country}"
                            change="onCountrySelect"
                            selectedKey="{oSupplierDetailModel>/vendorAddress/countryKey}"
                            valueState="{oMandatoryModel>/vendorAddress/countryKeyState}"
                            valueStateText="{i18n>ManFields}"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            showSecondaryValues="true"
                            selectionChange="onAddingmandvalue"
                        >
                            <core:ListItem key="{oDropdownModel>countryKey}" text="{oDropdownModel>countryKey}" additionalText="{oDropdownModel>countryName}" />
                        </ComboBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Plant" class="voGridLabel" required="true" />
                        <HBox renderType="Bare">
                            <ComboBox
                                class="sigComboBox"
                                width="50%"
                                items="{oDropdownModel>/phoneCountryCode}"
                                showSecondaryValues="true"
                                change="onChangeCountryCode"
                                selectedKey="{oSupplierDetailModel>/countryCode}"
                                valueState="{oMandatoryModel>/phoneCountrycodeState}"
                                valueStateText="{i18n>ManFields}"
                                selectionChange="onAddingmandvalue"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            >
                                <core:ListItem key="{oDropdownModel>countryCode}" text="{oDropdownModel>countryCode}" additionalText="{oDropdownModel>countryKey}" />
                            </ComboBox>
                            <Input
                                class="voInputText"
                                value="{oSupplierDetailModel>/mobileNumber}"
                                valueState="{oMandatoryModel>/mobileNumberState}"
                                valueStateText="{i18n>ManFields}"
                                liveChange="onPhoneNumberChange"
                                enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            />
                        </HBox>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Supplier Subrange" class="voGridLabel" />
                        <Input class="voInputText" value="{oSupplierDetailModel>/fax}" maxLength="40" enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}" />
                    </VBox>
                
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Supplier Currency" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street4}"
                            maxLength="40"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="street4" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Acknowledgement Required" class="voGridLabel" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/street5}"
                            maxLength="40"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                        >
                            <customData>
                                <core:CustomData key="target" value="street5" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="GR-Based IV" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/city}"
                            maxLength="40"
                            valueState="{oMandatoryModel>/vendorAddress/cityState}"
                            valueStateText="{i18n>ManFields}"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            liveChange="onAddingmandvalue"
                        >
                            <customData>
                                <core:CustomData key="target" value="city" />
                            </customData>
                        </Input>
                    </VBox>
                    <VBox class="sapUiSmallMarginTop">
                        <Label text="Mode of Transport - Border" class="voGridLabel" required="true" />
                        <Input
                            class="voInputText"
                            value="{oSupplierDetailModel>/vendorAddress/city}"
                            maxLength="40"
                            valueState="{oMandatoryModel>/vendorAddress/cityState}"
                            valueStateText="{i18n>ManFields}"
                            change="oAddressChange"
                            enabled="{parts: ['oSupplierDetailModel>/notExtended','oSupplierDetailModel>/isChanged'], formatter:'.formatter.setInputenbled'}"
                            liveChange="onAddingmandvalue"
                        >
                            <customData>
                                <core:CustomData key="target" value="city" />
                            </customData>
                        </Input>
                    </VBox>
                    </l:Grid>
            </VBox>
        </Panel>
    </VBox>
</core:FragmentDefinition>