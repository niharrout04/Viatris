<?xml version="1.0" encoding="UTF-8"?>
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
    <VBox>
        <Label text="BANK DETAILS" />
        <VBox />
        <l:Grid defaultSpan="XL3 L3 M6 S12" id="idSupDet288">
