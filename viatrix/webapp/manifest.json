{
  "_version": "1.49.0",
  "sap.app": {
    "id": "viatrix",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.0",
      "toolsId": "24dcf6f6-5fdb-4e35-b111-dfe421cc320c"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.111.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "viatrix.i18n.i18n"
        }
      },
      "oActivityLog": {
          "type": "sap.ui.model.json.JSONModel",
          "settings": {
              "defaultBindingMode": "TwoWay"
          }
      },
      "oButtonmodel": {
          "type": "sap.ui.model.json.JSONModel",
          "settings": {
              "defaultBindingMode": "TwoWay"
          }
      },
      "oCommonModel": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
            "defaultBindingMode": "TwoWay"
        }
    }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "viatrix.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "SearchVendor",
          "pattern": ":?query:",
          "target": [
            "TargetSearchVendor"
          ]
        },
        {
          "name": "VendorRegistrationForm",
          "pattern": "VendorRegistrationForm",
          "target": [
            "TargetVendorRegistrationForm"
          ]
        },
        {
          "name": "TaskDetails",
          "pattern": "TaskDetails",
          "target": [
            "TargetTaskDetails"
          ]
        },
        {
          "name": "VendorDetailOverview",
          "pattern": "VendorDetailOverview",
          "target": [
            "TargetVendorDetailOverview"
          ]
        }
      ],
      "targets": {
        "TargetSearchVendor": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "SearchVendor",
          "viewName": "SearchVendor"
        },
        "TargetVendorRegistrationForm": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "VendorRegistrationForm",
          "viewName": "VendorRegistrationForm"
        },
        "TargetTaskDetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "TaskDetails",
          "viewName": "TaskDetails"
        },
        "TargetVendorDetailOverview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "VendorDetailOverview",
          "viewName": "VendorDetailOverview"
        }
      }
    },
    "rootView": {
      "viewName": "viatrix.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
